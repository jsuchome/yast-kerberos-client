.etc.krb5_conf

`ag_ini(
  IniAgent(
   "/etc/krb5.conf",
    $[
      "options" : [ ],
      "comments": [ "^[ \t]*#.*", "#.*", "^[ \t]*$" ],
      "sections" : [
        $[
        "begin" : [ "^[ \t]*\\[[ \t]*(.*[^ \t])[ \t]*\\][ \t]*", "[%s]" ],
        ],
        $[
        "begin" : [ "^[ \t]*(.*[^ \t])[ \t]*=[ \t]*\\{[ \t]*", "%s = {" ],
        "end" : [ "^[ \t]*[ \t]*\\}[ \t]*", "}" ]
        ]
      ],
      "subindent": "\t",
      "params" : [
        $[
        "match" : [ "^[ \t]*([^=]*[^ \t=])[ \t]*=[ \t]*(.*[^ \t]|)[ \t]*$" , "%s = %s"],
      ],
    ],
    ]
  )
)
