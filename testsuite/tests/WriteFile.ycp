{
    // testedfiles: Kerberos.ycp
    import "Testsuite";
    import "Kerberos";

    path p = .etc.krb5_conf.v.libdefaults.default_realm;

    Testsuite::Test(``(Kerberos::WriteFile (p, "SUSE")), [ $[], $[], $[] ], 0);

    Testsuite::Test(``(Kerberos::WriteFile (p, "")), [ $[], $[], $[] ], 0);

    Testsuite::Test(``(Kerberos::WriteFile (p, nil)), [ $[], $[], $[] ], 0);
}
